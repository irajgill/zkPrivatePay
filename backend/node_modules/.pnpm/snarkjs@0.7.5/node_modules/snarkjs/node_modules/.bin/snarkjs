#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/raj/Desktop/zkprivatepay/backend/node_modules/.pnpm/snarkjs@0.7.5/node_modules/snarkjs/build/node_modules:/home/raj/Desktop/zkprivatepay/backend/node_modules/.pnpm/snarkjs@0.7.5/node_modules/snarkjs/node_modules:/home/raj/Desktop/zkprivatepay/backend/node_modules/.pnpm/snarkjs@0.7.5/node_modules:/home/raj/Desktop/zkprivatepay/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/raj/Desktop/zkprivatepay/backend/node_modules/.pnpm/snarkjs@0.7.5/node_modules/snarkjs/build/node_modules:/home/raj/Desktop/zkprivatepay/backend/node_modules/.pnpm/snarkjs@0.7.5/node_modules/snarkjs/node_modules:/home/raj/Desktop/zkprivatepay/backend/node_modules/.pnpm/snarkjs@0.7.5/node_modules:/home/raj/Desktop/zkprivatepay/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/cli.cjs" "$@"
else
  exec node  "$basedir/../../build/cli.cjs" "$@"
fi
